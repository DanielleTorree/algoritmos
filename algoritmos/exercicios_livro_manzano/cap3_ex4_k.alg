algoritmo "semnome"
// Data : 08/12/2020
// Seção de Declarações 
var
   DARB, D, C: real
inicio
      escreval("Conversão de Dólar para Real")
      escreva("Valor em Dólar que você possui: ")
      leia(D)
      escreva("1 Dólar equivale a quantos Reias: ")
      leia(DARB)
      C <- DARB*D
      escreva("O valor que você possui convertido para Real é R$", C)
      
      

fimalgoritmo
