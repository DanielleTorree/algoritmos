algoritmo "produto_soma"
// Data : 09/12/2020
// Seção de Declarações 
var
   A, B, C, D, P, S: inteiro
inicio
      escreva("Valor A: ")
      leia(A)
      escreva("Valor B: ")
      leia(B)
      escreva("Valor C: ")
      leia(C)
      escreva("Valor D: ")
      leia(D)
      P <- A*C
      S <- B+D
      escreva("O produto AxC = ", P, " e a soma B+C = ", S)
fimalgoritmo
