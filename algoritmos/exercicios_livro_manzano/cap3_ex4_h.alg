algoritmo "vol_caixa_retangular"
// Data : 08/12/2020
// Seção de Declarações 
var
   V, C, L, H: real
inicio
      escreval("Calculando de uma caixa retangular")
      escreva("Comprimento: ")
      leia(C)
      escreva("Largura: ")
      leia(L)
      escreva("Altura: ")
      leia(H)
      V <- C*L*H
      escreval("V = CxLxH")
      escreva("V = ", V)
fimalgoritmo
