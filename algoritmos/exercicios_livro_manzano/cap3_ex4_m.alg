algoritmo "soma_quadrados_lidos"
// Função :
// Autor :
// Data : 09/12/2020
// Seção de Declarações 
var
   A, B, C, R: inteiro
inicio
      escreva("Valor A: ")
      leia(A)
      escreva("Valor B: ")
      leia(B)
      escreva("Valor C: ")
      leia(C)
      R <- (A*A) + (B*B) + (C*C)
      escreva("A soma dos quadrados dos três valores lidos é: ", R)
fimalgoritmo
