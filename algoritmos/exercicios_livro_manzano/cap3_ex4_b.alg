algoritmo "conversão_temperatura_f_para_C"
// Função :
// Autor :
// Data : 07/12/2020
// Seção de Declarações 
var
   C, F: real
inicio
      escreva("Qual a temperatura em Fahrenheit: ")
      leia(F)
      C <-((F-32)*5)/9
      escreva("Esta mesma temperatura em Celsius é: ", C, "ºC")
fimalgoritmo
