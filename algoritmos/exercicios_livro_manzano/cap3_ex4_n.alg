algoritmo "quadrado_da_soma_valores"
// Função :
// Autor :
// Data : 09/12/2020
// Seção de Declarações 
var
   A, B, C, R: inteiro
inicio
      escreva("Valor A: ")
      leia(A)
      escreva("Valor B: ")
      leia(B)
      escreva("Valor C: ")
      leia(C)
      R <- (A + B + C)*(A + B + C)
      escreva("O valor do quadrado da soma dos três valores é: ", R)
fimalgoritmo
