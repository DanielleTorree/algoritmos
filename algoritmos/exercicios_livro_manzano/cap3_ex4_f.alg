algoritmo "a_para_b_e_b_para_a"
// Função :
// Autor :
// Data : 08/12/2020
// Seção de Declarações 
var
   A, B, C: inteiro
inicio
      escreva("VAR A = ")
      leia(A)
      escreva("VAR B = ")
      leia(B)
      C <- A
      A <- B
      B <- C

      escreva("VAR A = ", A, "; VAR B = ", B)
fimalgoritmo

