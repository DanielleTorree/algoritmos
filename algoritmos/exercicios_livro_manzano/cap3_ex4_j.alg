algoritmo "diferenca_valores_quadrado"

// Data : 08/12/2020
// Seção de Declarações 
var
   A, B, D: real
inicio
      escreva("Digite um valor: ")
      leia(A)
      escreva("Digite outro valor: ")
      leia(B)
      D <- A*A - B*B
      escreva("A diferença dos valores ao quadrado é ", D)
fimalgoritmo
